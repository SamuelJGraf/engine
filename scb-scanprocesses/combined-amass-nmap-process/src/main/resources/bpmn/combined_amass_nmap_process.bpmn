      <bpmn:outgoing>SequenceFlow_0p5mwz6</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="ServiceTask_DoAmassScan" name="Run amass&#10;Â subdomain scan" camunda:asyncBefore="true" camunda:type="external" camunda:topic="subdomain_scan">
